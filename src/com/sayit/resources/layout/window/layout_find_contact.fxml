<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.sayit.ui.control.frame.FindContactController">
    <children>
        <HBox alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#close" text="Button"/>
                <TextField fx:id="nameField" HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" onAction="#search" text="Button"/>
            </children>
        </HBox>
        <ListView fx:id="contactListView" style="-fx-background-color: red;" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
